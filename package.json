{
  "name": "hejs",
  "homepage": "https://github.com/gurelbs/hejs#readme",
  "description": "open-source engine with API for Real-time questions In Hebrew.",
  "version": "0.0.14",
  "main": "./server/server.js",
  "scripts": {
    "start": "node ./server/server.js",
    "heroku-postbuild": "concurrently \"cd client\" \"npm install\" \"npm run build\"",
    "start-dev": "concurrently \"nodemon ./server/server.js\" \"cd client && npm start\"",
    "deploy": "concurrently \"git add .\" \"git commit -m \"updated\"\" \"git push origin main\" \"git push heroku main\""
  },
  "keywords": ["hebrew", "api", "questions", "real-time"],
  "author": "Gurel Ben Shabat",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "puppeteer": "^10.2.0"
  },
  "engines": {
    "node": "14.x"
  }
}
